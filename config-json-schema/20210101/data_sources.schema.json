{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/10gen/baas/importexport/service-config-data",
    "$ref": "#/$defs/serviceConfigData",
    "$defs": {
        "serviceConfigData": {
            "properties": {
                "id": {
                    "type": "string",
                    "maxLength": 12,
                    "minLength": 12
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "config": {
                    "type": "object"
                },
                "secret_config": {
                    "type": "object"
                },
                "version": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "name",
                "type",
                "config"
            ]
        }
    }
}
