{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/10gen/baas/importexport/custom-user-data-config",
    "$ref": "#/$defs/CustomUserDataConfig",
    "$defs": {
        "CustomUserDataConfig": {
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "mongo_service_id": {
                    "type": "string",
                    "maxLength": 12,
                    "minLength": 12
                },
                "mongo_service_name": {
                    "type": "string"
                },
                "database_name": {
                    "type": "string"
                },
                "collection_name": {
                    "type": "string"
                },
                "user_id_field": {
                    "type": "string"
                },
                "on_user_creation_function_id": {
                    "type": "string",
                    "maxLength": 12,
                    "minLength": 12
                },
                "on_user_creation_function_name": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "enabled"
            ]
        }
    }
}
